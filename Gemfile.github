source "https://rubygems.org"

# Use specific versions for better compatibility
gem "jekyll", "~> 3.9.3"
gem "github-pages", "~> 228"
gem "webrick"

# Jekyll plugins
group :jekyll_plugins do
  gem "jekyll-include-cache"
  gem "jekyll-paginate"
  gem "jekyll-sitemap"
  gem "jekyll-gist"
  gem "jekyll-feed"
  gem "jemoji"
end

gem "minimal-mistakes-jekyll" 